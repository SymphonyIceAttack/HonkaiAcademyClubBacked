{
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper"
        },
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "outputDirectory": ".",
    "public": true,
    "installCommand": "pnpm install",
    "env": {
        "DATABASE_URL": "file:./dev.db"
    }
}