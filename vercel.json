{
    "builds": {
        "src": "./index.js",
        "use": "@vercel/node"
    },
    "outputDirectory": "dist",
    "devCommand": "pnpm dev",
    "buildCommand": "npx prisma migrate deploy && npx prisma generate && pnpm build",
    "installCommand": "pnpm install",
    "env": {
        "DATABASE_URL": "file:./dev.db"
    }
}